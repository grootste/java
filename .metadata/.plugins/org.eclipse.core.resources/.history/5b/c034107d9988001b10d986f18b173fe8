		// TODO Auto-generated method stub
//selinium code-
		
		//Create driver object for chrome browser
		
				
		/*Class name= X
				X driver=new X();*/
		System.setProperty("webdriver.chrome.driver", "D:\\ProgramData\\projects\\java\\chromedriver.exe");
